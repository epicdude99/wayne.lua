local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()

local Window = OrionLib:MakeWindow({Name = "wayne.lua | dev build", HidePremium = false, SaveConfig = true, ConfigFolder = "GameControls"})

-- Teleportation Tab
local TeleportTab = Window:MakeTab({
    Name = "bring item",
    Icon = "rbxassetid://4483345998",
    PremiumOnly = false
})

local TeleportSection = TeleportTab:AddSection({
    Name = "bring item"
})

-- Dropdown for selecting models
local modelDropdown = TeleportSection:AddDropdown({
    Name = "item",
    Default = "choose item..",
    Options = {},
    Callback = function(selectedModelName)
        
        _G.selectedModelName = selectedModelName
    end    
})

-- Function to add models to the dropdown
local function AddModelsToDropdown()
    local models = {}
    for _, model in ipairs(workspace:GetChildren()) do
        if model:IsA("Model") then
            table.insert(models, model.Name)
        end
    end
    if #models > 0 then
        modelDropdown:Refresh(models, true) -- 
    else
        OrionLib:MakeNotification({
            Name = "no models found",
            Content = "no models available",
            Image = "rbxassetid://4483345998",
            Time = 5
        })
    end
end

TeleportSection:AddButton({
    Name = "launch tp",
    Callback = AddModelsToDropdown
})

TeleportSection:AddButton({
    Name = "teleport item",
    Callback = function()
        local model = workspace:FindFirstChild(_G.selectedModelName)
        if model and model:IsA("Model") and model.PrimaryPart then
            model:SetPrimaryPartCFrame(game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame)
            OrionLib:MakeNotification({
                Name = "teleported item successfully",
                Content = "teleported: " .. _G.selectedModelName,
                Image = "rbxassetid://4483345998",
                Time = 5
            })
        else
            OrionLib:MakeNotification({
                Name = "teleport error",
                Content = "could not teleport the model",
                Image = "rbxassetid://4483345998",
                Time = 5
            })
        end
    end
})

OrionLib:Init()
